<h1>Listing trainer_pokemons</h1>

<table>
  <thead>
    <tr>
      <th>Pokemon</th>
      <th>Trainer</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @trainer_pokemons.each do |trainer_pokemon| %>
      <tr>
        <td><%= trainer_pokemon.pokemon %></td>
        <td><%= trainer_pokemon.trainer %></td>
        <td><%= link_to 'Show', trainer_pokemon %></td>
        <td><%= link_to 'Edit', edit_trainer_pokemon_path(trainer_pokemon) %></td>
        <td><%= link_to 'Destroy', trainer_pokemon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trainer pokemon', new_trainer_pokemon_path %>
